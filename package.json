{
  "name": "beatrix",
  "version": "0.6.4",
  "module": "./src/index.ts",
  "type": "module",
  "scripts": {
    "build:all": "bun run build:vite && bun run build:server",
    "build:ci": "bun run lint && bun run build:all",
    "build:server": "bun run ./scripts/build-server.ts",
    "build:vite": "vite build",
    "dev": "concurrently \"bun dev:server\" \"bun dev:vite\"",
    "dev:debug": "concurrently \"DEBUG='b:*' bun dev:server\" \"bun dev:vite\"",
    "dev:eval-mode": "concurrently \"bun dev:server -e\" \"bun dev:vite\"",
    "dev:test-mode": "concurrently \"bun dev:server -t\" \"bun dev:vite\"",
    "dev:server": "mkdir -p ./notebook && bun run ./server/index.ts serve -n ./notebook",
    "dev:vite": "vite",
    "evals": "bun run ./server/index.ts evals",
    "f": "bun fix && bun lint && bun test",
    "fix": "eslint --fix && prettier --write .",
    "lint": "tsc && eslint && prettier --check .",
    "mcp": "bun run ./server/index.ts mcp",
    "serve": "bun run ./server/index.ts serve",
    "start": "bun run ./server/index.ts",
    "start:debug": "DEBUG='*' bun run ./server/index.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@tailwindcss/vite": "^4.1.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bun": "^1.2.10",
    "@types/debug": "^4.1.12",
    "@types/mime-types": "^2.1.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.1",
    "bun-types": "latest",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "spawn-rx": "^5.1.2",
    "vite": "^6.3.2"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@anaisbetts/commands": "0.3.2",
